body
{
	font-family:'Trebuchet MS';
    margin:0;
	background-image:url('bg.jpg');
	background-repeat:no-repeat;
	background-size:cover;
	background-size: 100vw 150vh;
}
.content{
	width:1030px;
	height:550px;
	margin-left:270px;
	margin-top:35px;
	
}
.options{
	margin-left:64.5%;
	width:315px;
	height:17px;
