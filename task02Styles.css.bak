body
{
	font-family:'Trebuchet MS';
	background-image:url('bg1.jpg');
	background-repeat:no-repeat;
	background-size:cover;
	margin:0;
	
	
	
	
	
}
.content{
	width:1030px;
	height:550px;
	margin-left:16%;
	margin-right:14.7%;
}
.options{
	margin-left:63.5%;
	width:300px;
	height:17px;
	background-color:#3C558D;
	margin-top:0;
	font-size:12px;
	padding-left:6px;
	padding-top:3px;
	
}
.options a{
	color:white;
	text-decoration:none;
}
.options a + a:before{
	content: " | ";
    color:skyblue;;
}
.first
{
outline: 0.2px solid rgba(255,255,255,0.15);
